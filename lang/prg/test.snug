

use 'lib/block.snug';
use 'lib/string.snug';

rout main
{
    trans 10 ~ block;

    sub string::print (
        block ~ 'hello world\n'
    );

    sub block::range (
        block ~ block,
        start ~ 0,
        end ~ 10
    );

