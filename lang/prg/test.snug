

use 'lib/block.snug';
use 'lib/string.snug';

rout main
{
    put len = 10;
    trans 10 ~ block;

    sub block::iota (
        block ~ block,
    );

    sub block::sum (
        block ~ block,
        sum ~ _
    );
    debug 'block sum: {_}';

    enum x @ i ~ block
    {
        debug 'block value {x} at index {i}';
    }


}

