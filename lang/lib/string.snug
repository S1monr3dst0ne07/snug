
use 'lib/block.snug';

rout string::print (
    in block;
){
    sub block::len ( block ~ block, len ~ len );
    put index = 0;

    lab;
        write block.index;
        put index = index + 1;
    jump ~ len > index;
}


rout string::len (
    in block;
    out len;
){
    put len = 0 - 1;

    lab;
        put len = len + 1;
    jump ~ block.len != 0;
}



